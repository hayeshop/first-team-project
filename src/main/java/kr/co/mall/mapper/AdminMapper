package kr.co.mall.mapper;

import java.util.ArrayList;

import kr.co.mall.vo.AdminVo;

public interface AdminMapper {
	public void write(AdminVo avo);
	public void write_ok(AdminVo avo);
	public ArrayList<AdminVo> list(int index);
	public AdminVo content(String id);
	public int isPwd(AdminVo avo);
	public void delete(int id);
	public AdminVo update(AdminVo avo);
	public void update_ok(AdminVo avo);
}
