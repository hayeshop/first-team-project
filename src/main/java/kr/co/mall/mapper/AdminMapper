import javax.servlet.http.HttpSession;

import org.springframework.ui.Model;

import kr.co.mall.vo.AdminVo;

public interface AdminMapper {
	public String write_ok(AdminVo avo,HttpServletResponse response,HttpSession session);
	public ArrayList<AdminVo> list(Model model);
	public void delete(int id);
	public String update(AdminVo avo,Model model);
	public void update_ok(AdminVo avo);
	

}
