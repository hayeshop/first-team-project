package kr.co.mall.service;

import javax.servlet.http.HttpServletRequest;
import javax.servlet.http.HttpSession;

import org.springframework.ui.Model;

import kr.co.mall.vo.CommunityVo;

public interface CommunityService {
	public String write_ok(CommunityVo cvo);
	public String list(Model model, CommunityVo cvo);
	public String content(Model model, HttpServletRequest request);
	public String delete(CommunityVo cvo, HttpSession session);
	public String update(CommunityVo cvo, Model model);
	public String update_ok(CommunityVo cvo);
} 
