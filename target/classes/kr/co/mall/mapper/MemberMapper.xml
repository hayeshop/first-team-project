<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.co.mall.mapper.MemberMapper">
	<insert id="member_input_ok" parameterType="kr.co.mall.vo.MemberVo">
		insert into(userid,pwd,pwd2,name,post,juso,juso_etc,gender,birth,telecom,phone,writeday) 
		values(#{userid},#{pwd},#{pwd2},#{name},#{post},#{juso},#{juso_etc},#{gender},#{birth},#{telecom},#{phone},now())
	</insert>
</mapper>